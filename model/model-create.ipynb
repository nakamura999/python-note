     "output_type": "execute_result"
    }
   ],
   "source": [
    "from sklearn import datasets, svm\n",
    "import joblib\n",
    "\n",
    "# アヤメのサンプルデータを読み込み\n",
    "iris = datasets.load_iris()\n",
    "\n",
    "# データを保存\n",
    "clf = svm.SVC()\n",
    "clf.fit(iris.data, iris.target)\n",
    "\n",
    "# 学習済データを保存\n",
    "joblib.dump(clf, 'iris.pkl', compress=True)"
   ]
  },
